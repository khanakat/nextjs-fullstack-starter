  },
  "bugs": {
    "url": "https://github.com/khanakat/nextjs-fullstack-starter/issues"
  },
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist",
    "analyze": "ANALYZE=true npm run build",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:switch:postgresql": "node scripts/switch-db.js postgresql",
    "db:switch:sqlite": "node scripts/switch-db.js sqlite",
    "db:setup": "node scripts/setup-database.js",
    "db:reset": "prisma migrate reset",
    "postgres:local:help": "scripts\\db\\postgres-local.bat help",
    "postgres:local:install": "scripts\\db\\postgres-local.bat install",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postgres:local:start": "scripts\\db\\postgres-local.bat start",
    "postgres:local:stop": "scripts\\db\\postgres-local.bat stop",
    "postgres:local:status": "scripts\\db\\postgres-local.bat status",
    "postgres:local:create-db": "scripts\\db\\postgres-local.bat create-db",
    "postgres:docker:help": "scripts\\db\\postgres-docker.bat help",
    "postgres:docker:start": "scripts\\db\\postgres-docker.bat start",
    "postgres:docker:stop": "scripts\\db\\postgres-docker.bat stop",
    "postgres:docker:restart": "scripts\\db\\postgres-docker.bat restart",
    "postgres:docker:status": "scripts\\db\\postgres-docker.bat status",
    "postgres:docker:logs": "scripts\\db\\postgres-docker.bat logs",
    "postgres:docker:shell": "scripts\\db\\postgres-docker.bat shell",
    "postgres:docker:reset": "scripts\\db\\postgres-docker.bat reset",
    "postinstall": "prisma generate"
  },
